Model DRRPCrate { // Name of actor in DECORATE
    // Define model
    Path "models"
    Model 0 "crate.md3" // Model index, model file
    Scale 10.0 10.0 12.0
    Offset 0.0 0.0 0.1

    // Define material
    Path "models"

    // Define animation
    
    Skin 0 "crateuv.png" // Model index, texture
    FrameIndex CRAT A 0 0 // sprite, frame, model index, model frame

    // Open
    // NOINTERPOLATION
    FrameIndex CRT2 B 0 0 // sprite, frame, model index, model frame
    FrameIndex CRT2 C 0 1 // sprite, frame, model index, model frame
    FrameIndex CRT2 D 0 2 // sprite, frame, model index, model frame
    FrameIndex CRT2 E 0 3 // sprite, frame, model index, model frame
    FrameIndex CRT2 F 0 4 // sprite, frame, model index, model frame
    FrameIndex CRT2 G 0 5 // sprite, frame, model index, model frame
    FrameIndex CRT2 H 0 6 // sprite, frame, model index, model frame

    // Explode
    // NOINTERPOLATION
    // FrameIndex CRT3 B 0 8 // sprite, frame, model index, model frame
    // FrameIndex CRT3 C 0 9 // sprite, frame, model index, model frame
    // FrameIndex CRT3 D 0 10 // sprite, frame, model index, model frame
    // FrameIndex CRT3 E 0 11 // sprite, frame, model index, model frame
    // FrameIndex CRT3 F 0 12 // sprite, frame, model index, model frame
    // FrameIndex CRT3 G 0 13 // sprite, frame, model index, model frame
    // FrameIndex CRT3 H 0 13 // sprite, frame, model index, model frame
}

